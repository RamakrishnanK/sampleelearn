<div class="row">
<div class="span6 offset3">
<% @user.each do |user| %>
<% if current_user.admin? %>
		
		
		<%   if user.name == current_user.name  %>
		
		<%= link_to "Edit",edit_user_registration_path(user) %>
		<% else %>
		<%= link_to "Edit",edit_user_path(user) %>
		
		
		<% end %>
<% end %>

<%= user.name %>
<%= user.email %><br/>

<% end %>
</div>
</div>